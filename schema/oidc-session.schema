objectclass ( 1.3.6.1.4.1.40805.3.2.1 NAME 'subjectSession'
        DESC 'OpenID Connect subject session'
        SUP top
        STRUCTURAL
        MUST ( sessionKey $
               sessionSubject $
               sessionAuthDate $
               sessionCreationDate $
               sessionLastAccessDate )
        MAY ( description $
              sessionACR $
              sessionAMR $
              sessionMaxLife $
              sessionAuthLife $
              sessionMaxIdle $
              sessionClaims $
              sessionRelyingParty $
              sessionData )
        X-ORIGIN 'Connect2id Session Store' )
objectclass ( 1.3.6.1.4.1.40805.3.2.2 NAME 'subjectIndex'
        DESC 'OpenID Connect subject session index'
        SUP top
        STRUCTURAL
        MUST ( sessionSubject $
               sessionN $
               sessionKey )
        MAY ( description )
        X-ORIGIN 'Connect2id Session Store' )
